	partition Dim_Date = calculated
		mode: import
		source =
				
				VAR Cal =
				    CALENDARAUTO(6)   -- FY ends in June (July 1 -> June 30)
				RETURN
				ADDCOLUMNS(
				    Cal,
				    "Year", YEAR([Date]),
				    "MonthNum", MONTH([Date]),
				    "Month", FORMAT([Date], "MMM"),
				    "Quarter", "Q" & INT( (MONTH([Date]) + 2) / 3 ),
				    "YearMonth", FORMAT([Date], "YYYY-MM"),
				    "Month Year", FORMAT([Date], "MMM yyyy"),
				
				    -- Sorting keys (important!)
				    "YearMonthKey", YEAR([Date]) * 100 + MONTH([Date]),
				
				    -- Fiscal Year (July -> June)
				    "FY Start Year", YEAR([Date]) - IF(MONTH([Date]) < 7, 1, 0),
				    "Fiscal Year",
				        "FY "
				        & FORMAT( YEAR([Date]) - IF(MONTH([Date]) < 7, 1, 0), "00")
				        & "-"
				        & FORMAT( YEAR([Date]) - IF(MONTH([Date]) < 7, 1, 0) + 1, "00")
				)

	annotation PBI_Id = ef395664aadf489fbb25f087e41def3c

